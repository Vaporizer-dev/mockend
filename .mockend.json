{
  "Todos": {
    "title": {
      "string": {}
    },
    "userId": {
      "belongsTo": "User"
    },
    "completed": {
      "boolean": {}
    }
  },
  "User": {
    "userId": {
      "int": {
        "min": 1,
        "max": 20
      }
    },
    "name": {
      "string": {}
    },
    "email": {
      "regexp": "#[a-z]{5,10}@[a-z]{5}\\.[a-z]{2,3}"
    },
    "age": {
      "int": {
        "min": 21,
        "max": 60
      }
    }
  }
}
